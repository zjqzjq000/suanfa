#include<iostream>

使用命名空间标准；
const int N=1e5+10;
整数[N]；
整型main（）{
整数n，q；
cin>>n>>q;
for(int i=0;i<n;i++){
cin>>a[i];
 }
while（q--）时{
整数b；
cin>>b;
int l=0,r=n-1;
while(l<r){
int mid=l+r>>1;
if(a[mid]>=b) r=mid;
else l=mid+1;
 }
if(a[r]!=b) cout<<"-1 -1"<<endl;//如果不等于k，输出-1 -1
其他{
cout<<r<<" ";
int l=0,r=n-1;//枚举最后一个小于等于k的数下标
while(l<r){
int mid=l+r+1>>1;
if(a[mid]<=b) l=mid;
else r=mid-1;
 }
cout<<l<<endl;
 }

 }
返回0；
 }
